<Window x:Class="iTunesRemote.Agent.MainWindow" xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation" xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml" Title="iTunes Remote Agent" Height="350" Width="525">
	<Grid>
		<ListBox Name="DebugOutput" HorizontalAlignment="Stretch" VerticalAlignment="Stretch">
			<ListBox.ItemTemplate>
				<DataTemplate>
					<TextBlock Text="{Binding}" HorizontalAlignment="Stretch"></TextBlock>
				</DataTemplate>
			</ListBox.ItemTemplate>
		</ListBox>
	</Grid>
</Window>